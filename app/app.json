{
  "expo": {
    "name": "MediCapt",
    "slug": "medicaptApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "privacy": "unlisted",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "click.medicapt.medicaptapp",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "package": "click.medicapt.medicaptapp"
    },
    "web": {
      "build": { "babel": { "include": ["@ui-kitten/components"] } }
    },
    "packagerOpts": {
      "assetExts": ["json", "yaml", "svg", "png", "jpg"]
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-image-picker",
        {
          "photosPermission": "Medicapt uses your camera to allow you to add photographic records to your forms."
        }
      ],
      "expo-localization"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "andrei-92",
            "project": "react-native",
            "SENTRY_AUTH_TOKEN": "f2d6766322c14bd4b385b29bb074db509ed7af7aa8d848e9ab3b7111ccef0ca1",
            "_comment": "SENTRY_AUTH_TOKEN changed from authToken"
          }
        }
      ]
    }
  }
}
