{
  "expo": {
    "name": "MediCapt",
    "slug": "medicaptApp",
    "ios": {
      "bundleIdentifier": "com.medicapt.medicaptapp"
    },
    "android": {
      "package": "com.medicapt.medicaptapp"
    },
    "privacy": "unlisted",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
      "packagerOpts": {
      "assetExts": [
        "json",
        "yaml",
        "svg",
        "png",
        "jpg"
      ]
    }
  }
}
