{
  "entry": ["App.js"],
  "rootDir": "./src",
  "aliases": {
    "screens/*": ["screens", "screens/*"],
    "components/*": ["components", "components/*"],
    "utils/*": ["utils", "utils/*"]
  },
  "ignorePatterns": [
    "**/node_modules/**",
    "**/*.tests.{js,jsx,ts,tsx}",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/tests/**",
    "**/__tests__/**",
    "**/*.d.ts",
    "**/*.native.*"
  ],
  "ignoreUnimported": [],
  "extensions": [
    ".ts",
    ".js",
    ".tsx",
    ".jsx",
    ".web.ts",
    ".web.js",
    ".web.tsx",
    ".web.jsx"
  ],
  "ignoreUnused": [
    "react",
    "react-dom",
    "react-native",
    "aws-amplify-react-native",
    "amazon-cognito-identity-js",
    "expo-gl",
    "expo-modules-core",
    "expo-splash-screen",
    "expo-status-bar",
    "react-native-modal-datetime-picker",
    "react-native-signature-canvas",
    "react-native-view-shot",
    "react-native-web",
    "@react-native-community/netinfo",
    "@react-navigation/native-stack",
    "webpack"
  ],
  "ignoreUnresolved": []
}
